
#include <iostream>

using namespace std;

int main()
{
	int den, mes, god, a, vek;
	cout << "Vvedite den\n";
	cin >> den;
	cout << "Vvedite mesiac\n";
	cin >> mes;
	cout << "Vvedite god\n";
	cin >> god;
	cout << "Vvedite vek\n";

	god = vek * 100 + god;

	if ((mes > 12) or (mes < 1))
	{
		cout << "ERROR";
	}
	int d = abs(god) % 4;
	if ((den < 1) or (den > 31))
	{
		cout << "ERROR";
	}
	if (mes == 2)
	{
		if (d == 0)
		{
			if ((den < 1) or (den > 29))
			{
				cout << "ERROR";
			}
		}
		else
		{
			if ((den < 1) or (den > 28))
			{
				cout << "ERROR";
			}
		}
	}
	if ((mes == 4 || mes == 6 || mes == 8 || mes == 11) && den >= 31)
	{
		cout << "ERROR";
	}
	string ned[7]{ "ponedelnik", "vtornik", "sreda", "chetverg", "piatnica", "subbota", "voskresenie" };
	if (mes < 3)
	{
		--god;
		mes += 10;
	}
	else
		mes -= 2;
	a = (((den + 31 * mes / 12 + god + god / 4 - god / 100 + god / 400) - 1) % 7);
	cout << ned[a];
	return 0;
}
